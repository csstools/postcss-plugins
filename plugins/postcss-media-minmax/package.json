{
	"name": "@csstools/postcss-media-minmax",
	"description": "Use the range notation in CSS media queries",
	"version": "2.0.0",
	"contributors": [
		{
			"name": "Antonio Laguna",
			"email": "antonio@laguna.es",
			"url": "https://antonio.laguna.es"
		},
		{
			"name": "Romain Menke",
			"email": "romainmenke@gmail.com"
		},
		{
			"name": "yisi"
		}
	],
	"license": "MIT",
	"funding": [
		{
			"type": "github",
			"url": "https://github.com/sponsors/csstools"
		},
		{
			"type": "opencollective",
			"url": "https://opencollective.com/csstools"
		}
	],
	"engines": {
		"node": ">=18"
	},
	"type": "module",
	"main": "dist/index.cjs",
	"module": "dist/index.mjs",
	"exports": {
		".": {
			"import": {
				"types": "./dist/index.d.ts",
				"default": "./dist/index.mjs"
			},
			"require": {
				"default": "./dist/index.cjs"
			}
		}
	},
	"files": [
		"CHANGELOG.md",
		"LICENSE.md",
		"README.md",
		"dist"
	],
	"dependencies": {
		"@csstools/css-calc": "^2.0.0",
		"@csstools/css-parser-algorithms": "^3.0.0",
		"@csstools/css-tokenizer": "^3.0.0",
		"@csstools/media-query-list-parser": "^3.0.0"
	},
	"peerDependencies": {
		"postcss": "^8.4"
	},
	"devDependencies": {
		"@csstools/postcss-tape": "*",
		"puppeteer": "^23.0.2"
	},
	"scripts": {
		"build": "rollup -c ../../rollup/default.mjs",
		"docs": "node ../../.github/bin/generate-docs/install.mjs && node ../../.github/bin/generate-docs/readme.mjs",
		"lint": "node ../../.github/bin/format-package-json.mjs",
		"prepublishOnly": "npm run build && npm run test",
		"test": "node --test",
		"test:rewrite-expects": "REWRITE_EXPECTS=true node --test"
	},
	"homepage": "https://github.com/csstools/postcss-plugins/tree/main/plugins/postcss-media-minmax#readme",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/csstools/postcss-plugins.git",
		"directory": "plugins/postcss-media-minmax"
	},
	"bugs": "https://github.com/csstools/postcss-plugins/issues",
	"keywords": [
		"media queries",
		"postcss-plugin",
		"range"
	],
	"csstools": {
		"cssdbId": "media-query-ranges",
		"exportName": "postcssMediaMinMax",
		"humanReadableName": "PostCSS Media MinMax",
		"specUrl": "https://www.w3.org/TR/mediaqueries-4/#mq-features"
	},
	"volta": {
		"extends": "../../package.json"
	}
}
