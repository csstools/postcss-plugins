{
	"$schema": "../../node_modules/@stryker-mutator/core/schema/stryker-schema.json",
	"mutate": [
		"src/**/*.ts"
	],
	"buildCommand": "npm run build",
	"testRunner": "command",
	"coverageAnalysis": "perTest",
	"tempDirName": "../../.stryker-tmp",
	"commandRunner": {
		"command": "npm run test:tape"
	},
	"thresholds": {
		"high": 100,
		"low": 100,
		"break": 100
	},
	"inPlace": true
}
