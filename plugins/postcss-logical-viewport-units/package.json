	"csstools": {
		"cssdbId": "logical-viewport-units",
		"exportName": "postcssLogicalViewportUnits",
		"humanReadableName": "PostCSS Logical Viewport Units",
		"specUrl": "https://www.w3.org/TR/css-values-4/#viewport-relative-units"
