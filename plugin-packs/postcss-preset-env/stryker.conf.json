{
	"$schema": "../../node_modules/@stryker-mutator/core/schema/stryker-schema.json",
	"mutate": [
		"src/lib/**/*.mjs"
	],
	"buildCommand": "rollup -c ../../rollup/default.mjs",
	"testRunner": "command",
	"coverageAnalysis": "perTest",
	"tempDirName": "../../.stryker-tmp",
	"commandRunner": {
		"command": "node ./src/test/test.mjs"
	},
	"thresholds": {
		"high": 100,
		"low": 100,
		"break": 100
	},
	"inPlace": true
}
