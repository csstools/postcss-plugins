{
	"color-profile": {
		"descriptors": {
			"components": {
				"syntax": "<ident>#"
			},
			"rendering-intent": {
				"syntax": "relative-colorimetric | absolute-colorimetric | perceptual | saturation"
			},
			"src": {
				"syntax": "<url>"
			}
		}
	},
	"counter-style": {
		"descriptors": {
			"additive-symbols": {
				"syntax": "[ <integer [0,∞]> && <symbol> ]#"
			},
			"fallback": {
				"syntax": "<counter-style-name>"
			},
			"negative": {
				"syntax": "<symbol> <symbol>?"
			},
			"pad": {
				"syntax": "<integer [0,∞]> && <symbol>"
			},
			"prefix": {
				"syntax": "<symbol>"
			},
			"range": {
				"syntax": "[ [ <integer> | infinite ]{2} ]# | auto"
			},
			"speak-as": {
				"syntax": "auto | bullets | numbers | words | spell-out | <counter-style-name>"
			},
			"suffix": {
				"syntax": "<symbol>"
			},
			"symbols": {
				"syntax": "<symbol>+"
			},
			"system": {
				"syntax": "cyclic | numeric | alphabetic | symbolic | additive | [ fixed <integer>? ] | [ extends <counter-style-name> ]"
			}
		}
	},
	"font-face": {
		"descriptors": {
			"ascent-override": {
				"syntax": "[ normal | <percentage [0,∞]> ]{1,2}"
			},
			"descent-override": {
				"syntax": "[ normal | <percentage [0,∞]> ]{1,2}"
			},
			"font-display": {
				"syntax": "auto | block | swap | fallback | optional"
			},
			"font-family": {
				"syntax": "<family-name>"
			},
			"font-feature-settings": {
				"syntax": "normal | <feature-tag-value>#"
			},
			"font-language-override": {
				"syntax": "normal | <string>"
			},
			"font-named-instance": {
				"syntax": "auto | <string>"
			},
			"font-size": {
				"syntax": "auto | [ <number> ]{1,2}"
			},
			"font-stretch": {
				"syntax": "<font-stretch-absolute>{1,2}"
			},
			"font-style": {
				"syntax": "auto | normal | italic | oblique [ <angle [-90deg,90deg]>{1,2} ]?"
			},
			"font-variation-settings": {
				"syntax": "normal | [ <string> <number> ]#"
			},
			"font-weight": {
				"syntax": "auto | <font-weight-absolute>{1,2}"
			},
			"font-width": {
				"syntax": "auto | <'font-width'>{1,2}"
			},
			"line-gap-override": {
				"syntax": "[ normal | <percentage [0,∞]> ]{1,2}"
			},
			"size-adjust": {
				"syntax": "<percentage [0,∞]>"
			},
			"src": {
				"syntax": "<font-src-list>"
			},
			"subscript-position-override": {
				"syntax": "[ normal | from-font | <percentage> ]{1,2}"
			},
			"subscript-size-override": {
				"syntax": "[ normal | from-font | <percentage [0,∞]> ]{1,2}"
			},
			"superscript-position-override": {
				"syntax": "[ normal | from-font | <percentage> ]{1,2}"
			},
			"superscript-size-override": {
				"syntax": "[ normal | from-font | <percentage [0,∞]> ]{1,2}"
			},
			"unicode-range": {
				"syntax": "<urange>#"
			}
		}
	},
	"font-feature-values": {
		"descriptors": {
			"@annotation": {
				"syntax": "@annotation { <declaration-list> }"
			},
			"@character-variant": {
				"syntax": "@character-variant { <declaration-list> }"
			},
			"@historical-forms": {
				"syntax": "@historical-forms { <declaration-list> }"
			},
			"@ornaments": {
				"syntax": "@ornaments { <declaration-list> }"
			},
			"@styleset": {
				"syntax": "@styleset { <declaration-list> }"
			},
			"@stylistic": {
				"syntax": "@stylistic { <declaration-list> }"
			},
			"@swash": {
				"syntax": "@swash { <declaration-list> }"
			},
			"font-display": {
				"syntax": "auto | block | swap | fallback | optional"
			}
		}
	},
	"font-palette-values": {
		"descriptors": {
			"base-palette": {
				"syntax": "light | dark | <integer [0,∞]>"
			},
			"font-family": {
				"syntax": "<family-name>#"
			},
			"override-colors": {
				"syntax": "[ <integer [0,∞]> <color> ]#"
			}
		}
	},
	"function": {
		"descriptors": {
			"result": {
				"syntax": "<declaration-value>?"
			}
		}
	},
	"page": {
		"descriptors": {
			"bleed": {
				"syntax": "auto | <length>"
			},
			"marks": {
				"syntax": "none | [ crop || cross ]"
			},
			"page-orientation": {
				"syntax": "upright | rotate-left | rotate-right"
			},
			"size": {
				"syntax": "<length [0,∞]>{1,2} | auto | [ <page-size> || [ portrait | landscape ] ]"
			}
		}
	},
	"position-try": {
		"descriptors": {
			"align-self": {
				"syntax": "<'align-self'> | anchor-center"
			},
			"block-size": {
				"syntax": "<'block-size'>"
			},
			"bottom": {
				"syntax": "<'bottom'>"
			},
			"height": {
				"syntax": "<'height'>"
			},
			"inline-size": {
				"syntax": "<'inline-size'>"
			},
			"inset": {
				"syntax": "<'inset'>"
			},
			"inset-block": {
				"syntax": "<'inset-block'>"
			},
			"inset-block-end": {
				"syntax": "<'inset-block-end'>"
			},
			"inset-block-start": {
				"syntax": "<'inset-block-start'>"
			},
			"inset-inline": {
				"syntax": "<'inset-inline'>"
			},
			"inset-inline-end": {
				"syntax": "<'inset-inline-end'>"
			},
			"inset-inline-start": {
				"syntax": "<'inset-inline-start'>"
			},
			"justify-self": {
				"syntax": "<'justify-self'> | anchor-center"
			},
			"left": {
				"syntax": "<'left'>"
			},
			"margin": {
				"syntax": "<'margin'>"
			},
			"margin-block": {
				"syntax": "<'margin-block'>"
			},
			"margin-block-end": {
				"syntax": "<'margin-block-end'>"
			},
			"margin-block-start": {
				"syntax": "<'margin-block-start'>"
			},
			"margin-bottom": {
				"syntax": "<'margin-bottom'>"
			},
			"margin-inline": {
				"syntax": "<'margin-inline'>"
			},
			"margin-inline-end": {
				"syntax": "<'margin-inline-end'>"
			},
			"margin-inline-start": {
				"syntax": "<'margin-inline-start'>"
			},
			"margin-left": {
				"syntax": "<'margin-left'>"
			},
			"margin-right": {
				"syntax": "<'margin-right'>"
			},
			"margin-top": {
				"syntax": "<'margin-top'>"
			},
			"max-block-size": {
				"syntax": "<'max-block-size'>"
			},
			"max-height": {
				"syntax": "<'max-height'>"
			},
			"max-inline-size": {
				"syntax": "<'max-inline-size'>"
			},
			"max-width": {
				"syntax": "<'max-width'>"
			},
			"min-block-size": {
				"syntax": "<'min-block-size'>"
			},
			"min-height": {
				"syntax": "<'min-height'>"
			},
			"min-inline-size": {
				"syntax": "<'min-inline-size'>"
			},
			"min-width": {
				"syntax": "<'min-width'>"
			},
			"right": {
				"syntax": "<'right'>"
			},
			"top": {
				"syntax": "<'top'>"
			},
			"width": {
				"syntax": "<'width'>"
			}
		}
	},
	"property": {
		"descriptors": {
			"inherits": {
				"syntax": "true | false"
			},
			"initial-value": {
				"syntax": "<declaration-value>?"
			},
			"syntax": {
				"syntax": "<string>"
			}
		}
	},
	"view-transition": {
		"descriptors": {
			"navigation": {
				"syntax": "auto | none"
			},
			"types": {
				"syntax": "none | <custom-ident>+"
			}
		}
	}
}